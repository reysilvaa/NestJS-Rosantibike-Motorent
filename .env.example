# Database Configuration
DATABASE_URL="postgresql://postgres:password@localhost:5432/rental_motor?schema=public"

# JWT Secret for Authentication
JWT_SECRET="rahasia-aman-untuk-jwt-authentication"
JWT_EXPIRES_IN="7d"

# Redis Configuration
REDIS_HOST="localhost"
REDIS_PORT="6379"
REDIS_PASSWORD=""

# BullMQ Configuration
QUEUE_PREFIX="rental_motor"

# Server Configuration
PORT=3000
NODE_ENV="development"
CORS_ORIGIN="http://localhost:3000"

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=100

# Compression
COMPRESSION_LEVEL=6

# Baileys WhatsApp Configuration
BAILEYS_SESSION_PATH="./storage/whatsapp-sessions"
ADMIN_WHATSAPP_NUMBER="6281234567890"

# WhatsApp configuration
ADMIN_WHATSAPP=6281234567890
WHATSAPP_SESSION_PATH=./storage/whatsapp-session
WHATSAPP_SEND_DEBUG=false

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret 