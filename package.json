{
  "name": "backend",
  "version": "0.0.1",
  "description": "Backend API untuk aplikasi rental motor",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.8.0",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" nest start --watch --preserveWatchOutput",
    "start:debug": "cross-env NODE_ENV=development DEBUG=* nest start --watch",
    "start:prod": "node dist/main",
    "start:cluster": "pm2-runtime start dist/main.js -i max",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js --env production",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs",
    "pm2:setup": "pm2 startup && pm2 save",
    "pm2:monit": "pm2 monit",
    "pm2:reload": "pm2 reload ecosystem.config.js --env production",
    "pm2:scale:max": "pm2 scale rental-backend max",
    "pm2:scale": "pm2 scale rental-backend",
    "kill:port": "npx kill-port 3000",
    "dev": "pnpm run kill:port && pnpm run start:dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:test": "eslint \"src/**/*.spec.ts\" --config .eslintrc.test.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "docs:generate": "nest generate swagger",
    "docs:serve": "swagger-ui-express",
    "seed:admin": "ts-node src/seed-admin.ts",
    "prisma:seed": "ts-node prisma/seed.ts",
    "scaffold:module": "ts-node scripts/scaffold-module.ts",
    "check:structure": "ts-node scripts/check-structure.ts",
    "fix:structure": "pnpx ts-node scripts/check-structure.ts && echo 'Jika ada modul yang tidak valid, gunakan: pnpm run scaffold:module -- <nama-modul>'"
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.0.12",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.0.7",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.0.12",
    "@prisma/client": "^6.5.0",
    "@types/multer": "^1.4.12",
    "@types/passport-jwt": "^4.0.1",
    "baileys": "^6.7.16",
    "bcrypt": "^5.1.1",
    "body-parser": "^2.1.0",
    "bullmq": "^5.44.3",
    "cache-manager": "^6.0.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.6.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^9.6.0",
    "pm2": "^5.3.1",
    "qrcode-terminal": "^0.12.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.4",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.56.0",
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.13",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.14",
    "@types/pino": "^7.0.4",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^48.0.1",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/__tests__/**"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
